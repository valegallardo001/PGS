generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id    Int    @id @default(autoincrement())
  nombre String
  email String @unique
}

model BroadAncestry {
  ID               Int    @id @default(autoincrement())
  Symbol           String @unique
  Display_category String
}